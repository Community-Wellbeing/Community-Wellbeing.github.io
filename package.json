{
  "name": "community-wellbeing",
  "version": "1.0.0",
  "description": "Website for Community Wellbeing - Fitness and wellness services in Pemba",
  "main": "index.html",
  "scripts": {
    "start": "live-server --port=3000 --open=/index.html",
    "dev": "live-server --port=3000 --open=/index.html --watch=.",
    "build": "echo 'Building project...' && npm run optimize",
    "optimize": "html-minifier --collapse-whitespace --remove-comments --minify-css true --minify-js true index.html -o dist/index.html",
    "serve": "http-server -p 8080 -o"
  },
  "keywords": [
    "fitness",
    "wellbeing",
    "health",
    "training",
    "wellness",
    "pemba",
    "mozambique"
  ],
  "author": "Community Wellbeing",
  "license": "MIT",
  "devDependencies": {
    "live-server": "^1.2.2",
    "http-server": "^14.1.1",
    "html-minifier": "^4.0.0",
    "tailwindcss": "^3.3.6",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32"
  },
  "dependencies": {
    "font-awesome": "^4.7.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}